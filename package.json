{
  "name": "federalist-uswds-gatsby",
  "description": "A Federalist template using USWDS 2.0 + Gatsby",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf .cache public _site",
    "build": "gatsby build",
    "reset": "npx rimraf package-lock.json LICENSE.md CONTRIBUTING.md degit.json .git",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "federalist": "npm run federalist:build && npm run federalist:copy",
    "federalist:build": "gatsby build --prefix-paths",
    "federalist:copy": "mkdir -p _site && rm -rf _site/* && cp -r public/* _site/"
  },
  "dependencies": {
    "gatsby": "^2.20.23",
    "gatsby-awesome-pagination": "^0.3.5",
    "gatsby-plugin-manifest": "^2.3.4",
    "gatsby-plugin-netlify-cms": "^4.2.3",
    "gatsby-plugin-offline": "^3.1.3",
    "gatsby-plugin-react-helmet": "^3.2.3",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-source-filesystem": "^2.2.3",
    "gatsby-transformer-remark": "^2.7.2",
    "netlify-cms-app": "^2.11.14",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.0",
    "react-helmet": "^6.0.0",
    "uswds": "^2.6.0"
  },
  "devDependencies": {
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2"
  }
}
